SYSS:

rm imx-boot.bin
rm uc.lst
rm *.img
rm -fr iot-image-builds
sudo -u an git clone -b uc20-firecpu git+ssh://kiya956@git.launchpad.net/~shiner-team/shiner/+git/iot-image-builds
cp creds iot-image-builds
cd iot-image-builds
sudo -u an JOB_NAME=iot-shiner-uc20 MILESTONE=x99 PROJECT=shiner BUILD_ID=999 BOT_USERNAME=kiya956 LAUNCHPAD_TEAM=shiner-team LAUNCHPAD_PROJECT=shiner ./build-core.sh --lib-path=$(pwd)/build-core-function.sh --output-dir=deploy
cp deploy/uc.lst ../
cp deploy/imx-boot.bin ../
cp deploy/iot-shiner-uc20-*.img.xz ../
cd ../
sudo unxz iot-shiner-uc20-*.img.xz

SYSEND:
FLASH
INIT_LOGIN
CHECKBOX test-runner-imx8fire
PERIODIC day
