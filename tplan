SYSS:

rm imx-boot.bin
rm uc.lst
rm *.img
sudo rm -fr iot-image-builds
git clone -b uc20-firecpu git+ssh://kiya956@git.launchpad.net/~shiner-team/shiner/+git/iot-image-builds
cp creds iot-image-builds
cd iot-image-builds
JOB_NAME=iot-shiner-uc20 MILESTONE=daily PROJECT=shiner BUILD_ID=xxx BOT_USERNAME=kiya956 LAUNCHPAD_TEAM=shiner-team LAUNCHPAD_PROJECT=shiner ./build-core.sh --lib-path=$(pwd)/build-core-function.sh --output-dir=deploy
cp deploy/uc.lst ../
cp deploy/imx-boot.bin ../
cp deploy/iot-shiner-uc20-*.img.xz ../
cd ../
sudo unxz iot-shiner-uc20-*.img.xz

SYSEND:
SYSS:
usbrelay S2OD2_1=0
sleep 3
usbrelay S2OD2_1=1
SYSEND:
DEPLOY
